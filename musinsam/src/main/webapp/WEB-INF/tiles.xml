<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
       "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
       
<tiles-definitions>
	 
	 	<definition name="inner" template="/WEB-INF/layouts/main/templateInner.jsp">
		<put-attribute name="header" value="/WEB-INF/layouts/main/headerInner.jsp"></put-attribute>
		<put-attribute name="body" value="/WEB-INF/layouts/main/bodyInner.jsp"></put-attribute>
		<put-attribute name="footer" value="/WEB-INF/layouts/main/footerInner.jsp"></put-attribute>
	</definition>
	
	<definition name="WILDCARD:inner/*" extends="inner">
		<put-attribute name="body" value="/WEB-INF/view/inner/{1}.jsp"></put-attribute>
	</definition>
	
	<definition name="main" template="/WEB-INF/layouts/main/template.jsp">
		<put-attribute name="header" value="/WEB-INF/layouts/main/header.jsp"></put-attribute>
		<put-attribute name="today" value="/WEB-INF/layouts/main/today.jsp"></put-attribute>
		<put-attribute name="best" value="/WEB-INF/layouts/main/best.jsp"></put-attribute>
		<put-attribute name="footer" value="/WEB-INF/layouts/main/footer.jsp"></put-attribute>
	</definition>
	<definition name="WILDCARD:*/*" extends="main">
		<put-attribute name="best" value="/WEB-INF/view/{1}/{2}.jsp"></put-attribute>
	</definition>
	 



  
</tiles-definitions>

