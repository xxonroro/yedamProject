<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.yedam.question.QuestionMapper">
  	<select id ="selectQa" resultType="com.yedam.question.QuestionVO">
  		select *
  		from question
  	</select>
  	<select id="searchQa" parameterType ="String" resultType="com.yedam.question.QuestionVO">
  		select *
  		from question
  		where title like '%'||#{content}||'%'
  		
  	</select>
  </mapper>